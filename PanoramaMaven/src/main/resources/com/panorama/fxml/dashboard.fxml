          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <WebView fx:id="whenVaccinated" prefHeight="115.0" prefWidth="300.0" GridPane.rowSpan="2" />
            <WebView fx:id="statsView" prefHeight="155.0" prefWidth="300.0" GridPane.rowIndex="2" />
            <WebView fx:id="newsView" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowSpan="3" />
